{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "semi": "warn",
    "no-multiple-empty-lines": ["warn", { "max": 1 }],
    "quotes": [2, "single"],
    "comma-dangle": ["warn", "always-multiline"]
  }
}
